{
  "format": "custom",
  "id": "story",
  "label": "story",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "config": {
          "label": "UID"
        },
        "type": "UID"
      },
      "chapterTitle": {
        "type": "StructuredText",
        "config": {
          "label": "chapterTitle",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "heading1,heading2,heading3,heading4,heading5,heading6"
        }
      },
      "chapterIllustration": {
        "type": "Image",
        "config": {
          "label": "chapterIllustration",
          "constraint": {},
          "thumbnails": []
        }
      },
      "previousChapter": {
        "type": "Link",
        "config": {
          "label": "previousChapter",
          "select": "document"
        }
      },
      "nextChapter": {
        "type": "Link",
        "config": {
          "label": "nextChapter",
          "select": "document"
        }
      },
      "contents": {
        "type": "StructuredText",
        "config": {
          "label": "contents",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      }
    }
  }
}
